@model IEnumerable<Filter_Records_Between_Two_Dates.Models.User2>

@{
    ViewBag.Title = "Index";
}
<table>
    <tr>
        <td>@Html.TextBox("Entervalue")</td>
        <td><button type="submit" id="search">Search</button></td>
    </tr>
</table>
<div id="result"></div>
@section scripts{
    <script>
        $("#search").click(function () {
            $.ajax({
                url: "@Url.Action("Search","Default")",
                type:"POST",
                data: { value: $("#Entervalue").val() },
                success: function (data) {
                    console.log(data)
                    $("#result").html(data);
                }
            });
        });
    </script>
}