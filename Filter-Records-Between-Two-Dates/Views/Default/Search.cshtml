@model IEnumerable<Filter_Records_Between_Two_Dates.Models.User2>
@{
    ViewBag.Title = "Search";
}

@if (Model.Count() != 0)
{
    <table>

        @foreach (var item in Model)
        {
            <tr>
           
                <td>&rarr;@Html.DisplayTextFor(m => item.name)</td>
            </tr>
        }
    </table>
}
else
{
    <p>No data</p>
}